{
  "manifest_version": 3,
  "name": "Bookmark Navigator",
  "version": "1.0",
  "permissions": ["bookmarks", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
